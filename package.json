{
  "name": "knexfile",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-academy-challenges/boilerplate-react-webpack.git"
  },
  "main": "server/index.js",
  "scripts": {
    "dev": "nodemon --watch server server/index & webpack --watch",
    "dev-client": "webpack-dev-server --hot --inline --content-base public",
    "dev-server": "nodemon --watch server server/index",
    "test": "ava -v 'test/**/*.test.js'",
    "start": "node server/index",
    "knex": "knex",
    "sqlite3": "sqlite3",
    "setup": "npm run knex migrate:rollback && npm run knex migrate:latest && npm run knex seed:run"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.18.0",
    "enzyme": "^2.8.2",
    "jsdom": "^11.0.0",
    "nodemon": "^1.11.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "supertest": "^3.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bulma": "^0.6.0",
    "config": "^1.27.0",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.13.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sodium": "^2.0.1",
    "sqlite3": "^3.1.13",
    "superagent": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/dev-academy-challenges/boilerplate-react-webpack/issues"
  },
  "homepage": "https://github.com/dev-academy-challenges/boilerplate-react-webpack#readme",
  "directories": {
    "test": "test"
  }
}
